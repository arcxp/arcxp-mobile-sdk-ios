    "content_elements": [{
        "_id": "5UUF25ISI5DTHM647BYOW5VIFE",
        "type": "text",
        "additional_properties": {
            "_id": 1648047601885,
            "comments": [],
            "inline_comments": []
        },
        "content": "The ultimate productivity hack is saying no."
    }, {
        "_id": "APC4YE6LGNC7TLZHOTXR45TYHQ",
        "type": "text",
        "additional_properties": {
            "_id": 1648047601886,
            "comments": [],
            "inline_comments": []
        },
        "content": "Not doing something will always be faster than doing it. This statement reminds me of the old computer programming saying, “Remember that there is no code faster than no code.”"
    }, {
        "_id": "NJPYENDZ5FE53LG6EKUMYMARBU",
        "type": "text",
        "additional_properties": {
            "_id": 1648047601887,
            "comments": [],
            "inline_comments": []
        },
        "content": "Video:"
    }, {
